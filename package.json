{
  "name": "info-rincon",
  "version": "0.3.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "git-log-pretty": "git log --pretty=format:\"%h%x09%an%x09%ad%x09%s\"",
    "dev:ssr": "ng run info-rincon:serve-ssr",
    "serve:ssr": "node dist/functions/server/main.js",
    "build:ssr": "ng build && ng run info-rincon:server:production",
    "prerender": "ng run info-rincon:prerender",
    "postinstall": "npm run build:ssr",
    "deploy-firebase": "xcopy /E dist\\functions\\browser dist\\functions\\dist\\functions\\browser\\ /Y && xcopy /E dist\\functions\\server dist\\functions\\dist\\functions\\server\\ /Y && firebase deploy"
  },
  "private": true,
