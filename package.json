{
    "name": "info-rincon",
    "version": "0.1.5",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "deploy-firebase": "ng build --configuration production && ng deploy",
        "test": "ng test",
        "lint": "ng lint",
        "lint-fix": "npx ng lint info-rincon --fix",
        "e2e": "ng e2e",
        "dev:ssr": "ng run info-rincon:serve-ssr",
